{
    "architecture": "resnet50",
    "model_type": "ordinal",
    "loss": "coral",
    "batch_size": 8,
    "lr": 1e-5,
    "n_epochs": 75,
    "device": "cuda:0",
    "val_metric": "custom_cervix",
    "dropout_rate": 0.0,
    "is_weighted_loss": false,
    "is_weighted_sampling": false,
    "seed": 0,
    "train_frac": 0.65,
    "test_frac": 0.25,
    "train_transforms": {
        "Transpose": {"indices": [2, 0, 1]},
        "CenterSpatialCrop": {"roi_size":  [1024, 1024]},
        "Resize": {"spatial_size": [256, 256]},
        "ScaleIntensity": {},
        "RandRotate": {"range_x": 0.26, "prob": 0.5, "keep_size": true},
        "RandFlip": {"spatial_axis": 0, "prob": 0.5},
        "ToTensor": {}
    },
    "val_transforms": {
        "Transpose": {"indices": [2, 0, 1]},
        "CenterSpatialCrop": {"roi_size":  [1024, 1024]},
        "Resize": {"spatial_size": [256, 256]},
        "ScaleIntensity": {},
        "ToTensor": {}
    }
}
